#include <bits/stdc++.h>
#define ll long long int
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    ll r, o, c;
    cin >> r >> o >> c;
    ll rem = 20 - o;
    ll max = rem * 36;
    if ((max + c) > r)
    {
        cout << "YES\n";
    }
    else
    {
        cout << "NO\n";
    }
    return 0;
}
